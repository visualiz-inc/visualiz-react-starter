{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "babel-preset-gatsby"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "babel-plugin-transform-typescript-metadata",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-class-properties",
    ]
}